<?xml version='1.0' encoding='utf-8'?>
<widget id="com.compica.gasconverter" version="0.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <preference name="loglevel" value="DEBUG" />
    <preference name="AndroidLaunchMode" value="singleTop" />
    <feature name="App">
        <param name="android-package" value="org.apache.cordova.App" />
    </feature>
    <name>Gas Price Converter</name>
    <description>
        A simple application to convert the price of gas between liter and gallon.
    </description>
    <author email="info@compica.com" href="http://compica.com">
        Martin Fournier
    </author>
    <content src="index.html" />
    <access origin="*" />
</widget>
